aspect Java {

    syn String Function.getClassName();

    eq Function.getClassName() {
        final Attribute attribute = lookupAttribute("java").get();
        return ((StringLiteral)attribute.getArgument(0)).getValue();
    }

    syn String Function.getMethodName();

    eq Function.getMethodName() {
        final Attribute attribute = lookupAttribute("java").get();
        return ((StringLiteral)attribute.getArgument(1)).getValue();
    }

}
